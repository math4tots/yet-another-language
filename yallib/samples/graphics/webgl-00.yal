from 'html/document' import document
from 'html/htmlcanvaselement' import HTMLCanvasElement


document.body.innerHTML = "<canvas id='canvas' height=400 width=400/>"
const canvas = document.getElementById('canvas').get() as HTMLCanvasElement
const gl = canvas.getWebGLContext().get()

gl.clearColor(0, 0, 0.5, 1)
gl.clear(gl.COLOR_BUFFER_BIT)

