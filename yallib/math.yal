export as Math

export const PI = (native function(): Number "Math.PI")()
export const TAU "Two times PI" = 2 * PI

export native function sin "computes sine in radians" (x: Number): Number "Math.sin(x)"
export native function cos "computes cosine in radians" (x: Number): Number "Math.cos(x)"
export native function tan "computes tangent in radians" (x: Number): Number "Math.tan(x)"
