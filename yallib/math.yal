export as Math

export const PI = (native(): Number "Math.PI")()
export const TAU "Two times PI" = 2 * PI

export native sin "computes sine in radians" (x: Number): Number "Math.sin(x)"
export native cos "computes cosine in radians" (x: Number): Number "Math.cos(x)"
export native tan "computes tangent in radians" (x: Number): Number "Math.tan(x)"
